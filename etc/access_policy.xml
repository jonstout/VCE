<accessPolicy>

  <switch name="foobar"  ip="managementip" username="username" password="password">

    <port name="eth0/1" owner="edco">
      <tags start="1" end="100" workgroup="edco"/>
      <tags start="101" end="200" workgroup="ajco"/>
    </port>

   <port name="eth0/2" owner="edco">
      <tags start="1" end="100" workgroup="edco"/>
      <tags start="101" end="200" workgroup="ajco"/>
    </port>

  </switch>

  <workgroup name="edco"  description="this is edcos exchange point access workgroup">
    <user id="ebalas" name="Ed" email="ebalas@globalnoc.iu.edu" />
    <user id="stan" name="Stan" email="stan@globalnoc.iu.edu" />
  </workgroup>

  <workgroup name="ajco"  description="this is edcos exchange point access workgroup">
    <user id="aragusa" name="AJ" email="aragusa@globalnoc.iu.edu" />
  </workgroup>

  <workgroup name="admin" admin="1" description="admin workgroup">
    <user id="ebalas" name="Ed" email="ebalas@globalnoc.iu.edu" />
  </workgroup>


</accessPolicy>
